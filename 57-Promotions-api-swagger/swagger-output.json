{
  "swagger": "2.0",
  "info": {
    "title": "Promotions API",
    "description": "API для управления пользователями и акциями",
    "version": "1.0.0"
  },
  "host": "localhost:8000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {},
  "definitions": {
    "UserRegisterDto": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "example": "user@example.com"
        },
        "password": {
          "type": "string",
          "example": "Password123"
        },
        "name": {
          "type": "string",
          "example": "John Doe"
        },
        "role": {
          "type": "string",
          "example": "supplier"
        }
      }
    },
    "UserLoginDto": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "example": "user@example.com"
        },
        "password": {
          "type": "string",
          "example": "Password123"
        }
      }
    },
    "UserUpdatePasswordDto": {
      "type": "object",
      "properties": {
        "newPassword": {
          "type": "string",
          "example": "NewPassword123"
        }
      }
    },
    "PromotionCreateOrProposeDto": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "example": "New Promotion"
        },
        "description": {
          "type": "string",
          "example": "Promotion description"
        },
        "startDate": {
          "type": "string",
          "example": "2025-05-01T00:00:00Z"
        },
        "endDate": {
          "type": "string",
          "example": "2025-05-10T00:00:00Z"
        }
      }
    },
    "PromotionUpdateDto": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "example": "Updated Promotion"
        },
        "description": {
          "type": "string",
          "example": "Updated description"
        },
        "startDate": {
          "type": "string",
          "example": "2025-05-01T00:00:00Z"
        },
        "endDate": {
          "type": "string",
          "example": "2025-05-10T00:00:00Z"
        }
      }
    },
    "PromotionStatusDto": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string",
          "example": "APPROVED"
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ]
}